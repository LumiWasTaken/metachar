{
  "name": "metachar",
  "version": "0.0.1",
  "description": "Meta Character Manager",
  "author": "kubetnetes-bad",
  "private": false,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \\\"{src,apps,libs,test}/**/*.ts\\\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@astropub/codecs": "^0.4.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/typeorm": "^10.0.0",
    "@techstark/opencv-js": "^4.8.0-release.9",
    "axios": "^1.5.1",
    "axios-rate-limit": "^1.3.0",
    "axios-retry": "^3.8.0",
    "cheerio": "^1.0.0-rc.12",
    "exifreader": "^4.14.1",
    "jsquash-avif": "^1.0.2",
    "llama-tokenizer-js": "^1.1.3",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "mysql2": "^3.6.2",
    "node-libcurl": "^3.0.0",
    "png-chunk-text": "^1.0.0",
    "png-chunks-encode": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "smartcrop-sharp": "^2.0.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/consolidate": "^0.14.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.3.1",
    "@types/png-chunk-text": "^1.0.2",
    "@types/png-chunks-encode": "^1.0.1",
    "@types/png-chunks-extract": "^1.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
